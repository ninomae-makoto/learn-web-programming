{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5"
  },
  "include": [
    "./*",
    "./**/*",
  ],
  "exclude": [
    "~/.vscode",
    "~/.vscode/**/*",
    "~/node_modules/**/*"
  ]
}

{
  "name": "vue-element-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "serve": "vue-cli-service serve  --webpack-config vue.test.config.js",
    "build": "vue-cli-service build --webpack-config vue.test.config.js",
    "build:dev": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.config.js vue-cli-service build",
    "build:prod": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.prod.config.js vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --webpack-config vue.test.config.js"
  },
  "dependencies": {
    "element-ui": "^2.7.2",
    "express": "^4.16.4",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^8.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "lint-staged": "^8.1.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
